{
  "rules": {
    "valid-example": {
      "summary": "Examples must be valid against their defined schema.",
      "message": "\"{{property}}\" property {{error}}",
      "type": "validation",
      "given": "$..[?(@.example)]",
      "then": {
        "function": "schemaPath",
        "functionOptions": {
          "field": "example",
          "schemaPath": "$"
        }
      }
    }
  }
}
